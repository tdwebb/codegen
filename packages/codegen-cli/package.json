{
  "name": "@codegen/codegen-cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "CodeGen command-line interface",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codegen": "dist/bin/codegen.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts bin/codegen.ts --format esm --outDir dist --dts --shims",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
